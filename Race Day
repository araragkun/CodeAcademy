let raceNumber = Math.floor(Math.random() * 1000);
const earlyRegistered = true;
const runnerAge = 25;

if (runnerAge > 18 && earlyRegistered) {
// Add number or raceNumber = raceNumber +1000;

  raceNumber += 1000;
  console.log(
    `You will race at 9:30 am and your Race Number is ${raceNumber}.`
  );
} else if (runnerAge > 18 && !earlyRegistered) {
  console.log(
    `You will race at 11:00 am and your Race Number is ${raceNumber}.`
  );
} else if (runnerAge < 18) {
  console.log(
    `You will race at 12:30 pm and your Race Number is ${raceNumber}.`
  );
} else {
  console.log("Please refer to the registation desk.");
}
